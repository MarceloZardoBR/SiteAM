@model FiapAM.Models.Contato

@{
    ViewBag.Title = "VisualizarContato";
    Layout = "~/Views/Shared/LayoutWebAdm.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Consultar Motivo do Contato</title>
</head>
<body>
    <!-- Page Content -->
    <div class="container">
        <!-- Call to Action Well -->
        <div class="row">
            <div class="col-lg-12">
                <div class="well">
                    <h2>Investidor</h2>
                    <div class="form-group">
                        @Html.LabelFor(model => model.Nome, htmlAttributes: new { @class = "control-label" })
                        @Html.DisplayFor(model => model.Nome);
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.Mensagem, htmlAttributes: new { @class = "control-label" })
                        @Html.DisplayFor(model => model.Mensagem)
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.Email, htmlAttributes: new { @class = "control-label" })
                        @Html.DisplayFor(model => model.Email);
                    </div>
                    <hr>
                    @Html.ActionLink("Voltar", "Index", new object { }, new { @class = "btn btn-default btn-lg" })
                </div>
            </div>
            <!-- /.col-lg-12 -->
        </div>
    </div>
</body>
</html>
